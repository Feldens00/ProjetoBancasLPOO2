@model ProjetoBancaTCC_OO2.Models.Professor
@{
    ViewBag.Title = "CreateProfessor";
}

<h2>Adicionar Professor</h2>


<div class="panel">
    <form action="~/Professor/CreateProfessor" method="post">
        <div class="form-group">
            <label>id</label>
            <input class="form-control" type="text" id="Id" name="Id"  />
        </div>

        <div class="form-group">
            <label>Nome</label>
            <input class="form-control" placeholder="Nome da pessoa" type="text" id="Nome" name="Nome"  />
        </div>
        <button class="btn btn-default" type="submit">Salvar</button>
    </form>
</div>
